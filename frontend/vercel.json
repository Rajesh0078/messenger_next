{
    "version": 2,
    "builds": [
        {
            "src": "express/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "next.config.js",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/socket",
            "dest": "express/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}